# Agri-Voice Environment Variables
# Copy this file to .env in the client directory

# Backend API / Socket.io Server URL (Required for Community Hub)
VITE_API_URL=http://localhost:5000

# Firebase Configuration (Required for Google Auth)
# Get from: https://console.firebase.google.com/ > Project Settings > General > Your apps
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abc123def456
